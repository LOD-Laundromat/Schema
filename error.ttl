  rdfs:isDefinedBy error: ;
  rdfs:label "error message"@en-us ;
  rdfs:range xsd:string ;
.
error:missingTypeKeywordInMtreeSpec
  rdf:type error:ArchiveError ;
  error:code "2"^^xsd:nonNegativeInteger ;
