@prefix   xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix  rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix   rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix error: <http://lodlaundromat.org/error/ontology/> .

# Named toplevel resources (53)

error:Action
        rdfs:comment "An action performed by a computing device."@en ;
        rdfs:label "action"@en ;
        rdfs:subClassOf rdfs:Resource .

error:ArchiveError
        rdfs:comment "An error that occurs as part of an archive (un)packing process."@en ;
        rdfs:label "archive error"@en ;
        rdfs:subClassOf error:Error .

error:DirectoryExistenceError
        error:code 17 ;
        error:message "File exists"^^xsd:string ;
        error:variableName "EXIST"^^xsd:string ;
        rdfs:comment "An error that occurs because a directory which is assumed to exist does not."@en ;
        rdfs:label "directory existence error"@en ;
        rdfs:subClassOf error:ExistenceError .

error:DirectoryNotEmptyError
        error:code 39 ;
        error:message "Directory not empty"^^xsd:string ;
        error:variableName "ENOTEMPTY"^^xsd:string ;
        rdfs:comment "An error that occurs because a directory is assumed to be empty, but is not."@en ;
        rdfs:label "directory not empty"@en ;
        rdfs:subClassOf error:ExistenceError .

error:Error
        rdfs:comment "An recognized irregularity in program execution."@en ;
        rdfs:label "error"@en ;
        rdfs:subClassOf rdfs:Resource .

error:ExistenceError
        error:code 2 ;
        error:message "No such file or directory"^^xsd:string ;
        error:variableName "ENOENT"^^xsd:string ;
        rdfs:comment "An error that occurs because a resource which is assumed to exist does not."@en ;
        rdfs:label "existence error"@en ;
        rdfs:subClassOf error:Error .

error:FileExistenceError
        rdfs:comment "An error that occurs because a file which is assumed to exist does not."@en ;
        rdfs:label "file existence error"@en ;
        rdfs:subClassOf error:ExistenceError .

error:HttpError
        rdfs:comment "An error that is emitted as part of an HTTP reply."@en ;
        rdfs:label "HTTP error"@en ;
        rdfs:subClassOf error:Error .

error:HttpUri
        rdfs:comment "A URI that uses the HTTP scheme."@en ;
        rdfs:label "HTTP URI"@en ;
        rdfs:subClassOf error:Object .

error:IoError
        rdfs:comment "An error that was thrown due to an input and/or output issue."@en ;
        rdfs:label "IO error"@en ;
        rdfs:subClassOf error:Error .

error:IsADirectoryError
        error:code 21 ;
        error:message "Is a directory"^^xsd:string ;
        error:variableName "EISDIR"^^xsd:string ;
        rdfs:comment "???"@en ;
        rdfs:label "source sink error"@en ;
        rdfs:subClassOf error:ExistenceError .

error:MalformedUrl
        rdfs:comment "An error that is thrown for a value which is expected to be a URL\n     but does not conform to URL syntax."@en ;
        rdfs:label "malformed URL"@en ;
        rdfs:subClassOf error:Error .

error:MissingTurtlePrefixDefinition
        rdfs:comment "An error that occurs if a Turtle prefix is used but not defined."@en ;
        rdfs:label "missing Turtle prefix definition"@en ;
        rdfs:subClassOf error:TurtleParserError .

error:Object
        rdfs:comment "An object that is related to an error."@en ;
        rdfs:label "object"@en ;
        rdfs:subClassOf rdfs:Resource .

error:ParserError
        rdfs:comment "An error that is emitted by a parser, indicating deviation from a defined grammar."@en ;
        rdfs:label "parser error"@en ;
        rdfs:subClassOf error:Error .

error:PermissionError
        rdfs:comment "An error that occurs because a specific action is not allowed to be performed w.r.t. a specific object that is of a given type."@en ;
        rdfs:label "permission error"@en ;
        rdfs:subClassOf error:Error .

error:RdfParserError
        rdfs:comment "An error that is emitted by an RDF parser."@en ;
        rdfs:label "RDF parser error"@en ;
        rdfs:subClassOf error:ParserError .

error:RdfxmlParserError
        rdfs:comment "An that is emitted by an RDF/XML parser when it encounters data that does not conform to the RDF/XML grammar."@en ;
        rdfs:label "RDF/XML parser error"@en ;
        rdfs:subClassOf error:RdfParserError ,
                        error:XmlParserError .

error:RedefinedRdfId
        rdfs:comment "An error that is thrown whenever multiple definitions of the same resource occur in an RDF/XML document. This typically occurs whenever more than one rdf:Id statement occurs for the same resource."@en ;
        rdfs:label "redefined RDF identifier"@en ;
        rdfs:subClassOf error:RdfxmlParserError .

error:SgmlParserError
        rdfs:comment "A warning that is emitted by an SGML parser."@en ;
        rdfs:label "SGML parser error"@en ;
        rdfs:subClassOf error:ParserError .

error:SocketError
        rdfs:comment "An error thrown because of a socket-related problem."@en ;
        rdfs:label "socket error"@en ;
        rdfs:subClassOf error:Error .

error:StreamPosition
        rdfs:comment "A position in a stream."@en ;
        rdfs:label "stream position"@en ;
        rdfs:subClassOf error:Error .

error:SyntaxError
        rdfs:comment "A parser error that is due to the occurrence of an expression in the parsed data stream that does not conform to the expected grammar."@en ;
        rdfs:label "syntax position"@en ;
        rdfs:subClassOf error:ParserError .

error:TimeoutError
        rdfs:comment "An error that is thrown due to a timeout."@en ;
        rdfs:label "timeout error"@en ;
        rdfs:subClassOf error:Error .

error:TurtleParserError
        rdfs:comment "A warning that is emitted by an XML parser."@en ;
        rdfs:label "XML parser error"@en ;
        rdfs:subClassOf error:RdfParserError .

error:XmlParserError
        rdfs:comment "A warning that is emitted by an XML parser."@en ;
        rdfs:label "XML parser error"@en ;
        rdfs:subClassOf error:SgmlParserError .

error:action
        a rdf:Property ;
        rdfs:comment "The action to which the error is related."@en ;
        rdfs:domain error:Error ;
        rdfs:label "action"@en ;
        rdfs:range error:Action .

error:character
        a rdf:Property ;
        rdfs:comment "The character number of a stream position, indicating the number of characters that have preceded it."@en ;
        rdfs:domain error:StreamPosition ;
        rdfs:label "character"@en ;
        rdfs:range xsd:integer .

error:code
        a rdf:Property ;
        rdfs:comment "Error code as used by the program in which the error occurs."@en ;
        rdfs:domain error:Error ;
        rdfs:label "error code"@en ;
        rdfs:range xsd:integer .

error:column
        a rdf:Property ;
        rdfs:comment "The column number of a stream position, indicating the number of characters that have preceded it since the last newline character or since the beginning of the stream."@en ;
        rdfs:domain error:StreamPosition ;
        rdfs:label "column"@en ;
        rdfs:range xsd:integer .

error:connectionRefused
        a error:SocketError ;
        error:code 111 ;
        error:message "Connection refused"^^xsd:string ;
        error:variableName "ECONNREFUSED"^^xsd:string .

error:connectionResetByPeer
        a error:ExistenceError ,
          error:SocketError ;
        error:code 104 ;
        error:message "Connection reset by peer"^^xsd:string ;
        error:variableName "ECONNRESET"^^xsd:string .

error:connectionTimedOut
        a error:SocketError ;
        error:code 110 ;
        error:message "Connection timed out"^^xsd:string ;
        error:variableName "ETIMEDOUT"^^xsd:string .

error:hostNotFound
        a error:SocketError ;
        rdfs:label "Host not found"^^xsd:string .

error:illegalUtf8Continuation
        a error:IoError ;
        rdfs:comment "Illegal UTF-8 continuation."@en ;
        rdfs:label "illegal UTF-8 continuation"@en .

error:illegalUtf8Start
        a error:IoError ;
        rdfs:comment "Illegal UTF-8 start."@en ;
        rdfs:label "illegal UTF-8 start"@en .

error:isADirectory
        a error:IoError ;
        error:code 21 ;
        error:message "Is a directory"^^xsd:string ;
        error:variableName "EISDIR"^^xsd:string ;
        rdfs:comment "???"@en ;
        rdfs:label "is a directory"@en .

error:line
        a rdf:Property ;
        rdfs:comment "The line number of a stream position, indicating the number of newline characters that have preceded it."@en ;
        rdfs:domain error:StreamPosition ;
        rdfs:label "column"@en ;
        rdfs:range xsd:integer .

error:message
        a rdf:Property ;
        rdfs:comment "Error message for the human programmer."@en ;
        rdfs:domain error:Error ;
        rdfs:label "error message"@en ;
        rdfs:range xsd:string .

error:missingTypeKeywordInMtreeSpec
        a error:ArchiveError ;
        error:code 2 ;
        error:message "Missing type keyword in mtree specification."^^xsd:string .

error:noData
        a error:SocketError ;
        error:code 61 ;
        error:message "No data available"^^xsd:string ;
        error:variableName "ENODATA"^^xsd:string ;
        rdfs:label "No data"@en .

error:noRouteToHost
        a error:SocketError ;
        error:code 113 ;
        error:message "No route to host"^^xsd:string ;
        error:variableName "EHOSTUNREACH"^^xsd:string .

error:notATypewriter
        a error:IoError ;
        error:code 25 ;
        error:message "Inappropriate ioctl for device"^^xsd:string ;
        error:variableName "ENOTTY"^^xsd:string ;
        rdfs:comment "Inappropriate input/output control for device."@en ;
        rdfs:label "not a typewriter"@en .

error:object
        a rdf:Property ;
        rdfs:comment "The name of the object to which the error relates."@en ;
        rdfs:domain error:Error ;
        rdfs:label "object"@en ;
        rdfs:range error:Object .

error:readTimeoutError
        a error:TimeoutError ;
        rdfs:comment "Timeout occurs while reading"@en ;
        rdfs:label "read timeout error"@en .

error:redirectAction
        a error:Action ;
        rdfs:comment "A rediraction action."@en ;
        rdfs:label "redirect action"@en .

error:sourceLine
        a rdf:Property ;
        rdfs:comment "The line number in the original source file for which a parser warning was thrown."@en ;
        rdfs:domain error:ParserError ;
        rdfs:label "source line"@en ;
        rdfs:range xsd:integer .

error:sslError
        a error:Error ;
        rdfs:comment "SSL error"@en ;
        rdfs:label "SSL error"@en .

error:streamPosition
        a rdf:Property ;
        rdfs:comment "The stream position at which an error occurred."@en ;
        rdfs:domain error:Error ;
        rdfs:label "stream position"@en ;
        rdfs:range error:StreamPosition .

error:tryAgain
        a error:SocketError ;
        rdfs:label "Try again"^^xsd:string .

error:variableName
        a rdf:Property ;
        rdfs:comment "The C variable name of the error as used in POSIX or in a Linux library or application."@en ;
        rdfs:domain error:Error ;
        rdfs:label "variable name"@en ;
        rdfs:range xsd:string .

<http://lodlaundromat.org/http/ontology/3xx>
        rdfs:subClassOf error:HttpError .

<http://lodlaundromat.org/http/ontology/4xx>
        rdfs:subClassOf error:HttpError .
